{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "conorburke/fantasy-frontend",
      "hostname": "frontend",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "conorburke/fantasy-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "etl",
      "image": "conorburke/fantasy-etl",
      "hostname": "etl",
      "essential": false,
      "memory": 128
    },
    {
      "name": "database",
      "image": "conorburke/fantasy-database",
      "hostname": "database",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "conorburke/fantasy-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["frontend", "api"],
      "memory": 128
    }
  ]
}